{
  "version": 2,
  "builds": [
    {
      "src": "clerk-javascript/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/clerk-javascript/dist/index.html"
    }
  ]
}
